<appConfig xmlns="http://nbcr.sdsc.edu/opal/types"
           xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata  appName="cat">
    <usage><![CDATA[concatenate files and print on the standard output ]]></usage>
    <info xsd:type="xsd:string"> <![CDATA[ cat [-benstuv] [-] [file ...] ]]> </info>

     <types>
     <taggedParams>
        <param>
            <id>input</id>
            <tag>-input</tag>
            <ioType>INPUT</ioType>
            <paramType>FILE</paramType>
            <required>true</required>
            <textDesc>input file</textDesc>
        </param>
     </taggedParams>
     </types>

  </metadata>

  <binaryLocation>/share/apps/bin/cat.sh</binaryLocation>
  <parallel>false</parallel>
</appConfig>

